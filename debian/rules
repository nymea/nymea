#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1
export QT_QPA_PLATFORM=minimal

override_dh_auto_configure:
	qmake .

override_dh_auto_build:
	make
	make doc

override_dh_clean:
	dh_clean

override_dh_install:
	dh_install --fail-missing

%:
	dh $@ --parallel

